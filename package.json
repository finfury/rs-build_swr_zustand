        "@types/node": "^22.4.2",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "dotenv": "^16.4.5",
        "eslint": "^9.9.0",
        "eslint-plugin-react": "^7.35.0",
